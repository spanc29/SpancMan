<% title @dossier.reference %>
<%= render ('layouts/navigation') %>

<div class="twelve_column section">
<div class="ten column border">

<div class="outline">

<table>
  <tr>
    <th>Type Entretien</th>
    <th>Dossier</th>
    <th>Ouvrage</th>
    <th>Vidangeur</th>
    <th>Num Vehicule</th>
    <th>Num Bordereau</th>
    <th>Date Entretien</th>
    <th>Quantite</th>
    <th>Destination Boue</th>
    <th>Observations</th>
  </tr>
  <% for entretien in @entretiens %>
    <tr>
      <td><%= entretien.type_entretien %></td>
      <td><%= entretien.dossier_id %></td>
      <td><%= entretien.ouvrage %></td>
      <td><%= entretien.vidangeur %></td>
      <td><%= entretien.num_vehicule %></td>
      <td><%= entretien.num_bordereau %></td>
      <td><%= entretien.date_entretien %></td>
      <td><%= entretien.quantite %></td>
      <td><%= entretien.destination_boue %></td>
      <td><%= entretien.observations %></td>

    </tr>
  <% end %>
</table>
</div>
</div>

<div class="two column">
<div class="container box"><%= link_to "Ajouter un entretien", new_dossier_entretien_path(@dossier), :class => "ajout" %></div>
</div>
</div><!-- fin six col -->
