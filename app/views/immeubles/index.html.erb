<% title "Immeubles" %>

<table>
  <tr>
    <th>Numero Rue</th>
    <th>Adress</th>
    <th>Code Postal</th>
    <th>Commune</th>
    <th>Residence</th>
    <th>Taux Occupation</th>
    <th>Annee Constr</th>
    <th>Nb Piece Princ</th>
    <th>Nb Eq Hab</th>
    <th>Dossier</th>
  </tr>
  <% for immeuble in @immeubles %>
    <tr>
      <td><%= immeuble.numero_rue %></td>
      <td><%= immeuble.adress %></td>
      <td><%= immeuble.code_postal %></td>
      <td><%= immeuble.commune %></td>
      <td><%= immeuble.residence %></td>
      <td><%= immeuble.taux_occupation %></td>
      <td><%= immeuble.annee_constr %></td>
      <td><%= immeuble.nb_piece_princ %></td>
      <td><%= immeuble.nb_eq_hab %></td>
      <td><%= immeuble.dossier_id %></td>
      <td><%= link_to "Show", immeuble %></td>
      <td><%= link_to "Edit", edit_immeuble_path(immeuble) %></td>
      <td><%= link_to "Destroy", immeuble, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Immeuble", new_immeuble_path %></p>
