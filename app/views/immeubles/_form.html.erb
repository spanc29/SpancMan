<%= simple_form_for ([@dossier,@dossier.immeubles.new]) do |f| %>
  <p><%= f.input :numero_rue %></p>
  <p><%= f.input :adresse %></p>
  <p><%= f.input :code_postal %></p>
  <p><%= f.input :commune %></p>
  <p><%= f.input :residence %></p>
  <p><%= f.input :taux_occupation %></p>
  <p><%= f.input :commentaire %></p>
  <p><%= f.input :annee_constr%></p>
  <p><%= f.input :nb_piece_princ, :as => :select, :collection => 1..20 %></p>
  <p><%= f.input :nb_eq_hab, :as =>:select, :collection => 1..200 %></p>


  <%= f.simple_fields_for :compteurs do |compt| %>
    <%= render "compteurs/form", :f => compt %>
  <% end %>
  <p><%= link_to_add_fields "Ajouter aEP", f, :compteurs %></p>

  <p><%= f.submit %></p>
<% end %>
