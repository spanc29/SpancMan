<table>
  <tr>
    <th>Numero Rue</th>
    <th>Adress</th>
    <th>Code Postal</th>
    <th>Commune</th>
    <th>Residence</th>
    <th>Taux Occupation</th>
    <th>Annee Constr</th>
    <th>Nb Piece Princ</th>
    <th>Nb Eq Hab</th>
    <th>Dossier</th>
  </tr>
  <% for immeuble in @immeubles %>
    <tr>
      <td><%= immeuble.numero_rue %></td>
      <td><%= immeuble.adresse %></td>
      <td><%= immeuble.code_postal %></td>
      <td><%= immeuble.commune %></td>
      <td><%= immeuble.residence %></td>
      <td><%= immeuble.taux_occupation %></td>
      <td><%= immeuble.annee_constr %></td>
      <td><%= immeuble.nb_piece_princ %></td>
      <td><%= immeuble.nb_eq_hab %></td>
      <td><%= immeuble.dossier_id %></td>

    </tr>
  <% end %>

<h3>compteurs</h3>
<div id="comments">
  <%= render "compteurs/compteur" %>
</div>
<%= link_to "ajout compteurs", new_immeuble_compteur_path(@immeuble,:format => :js), :remote => true, :id => 'new_comment_link' %>
</table>
