<div class="outline">

<table>
  <thead>
  <tr>
    <th>N°</th>
    <th>Adresse</th>
    <th>liste Usager(s)</th>
    <th>type habitat</th>
    <th>taux occupation</th>
    <th>Annee Constr</th>
    <th>Année renov</th>
    <th>Nb piece princ</th>
    <th>Nb Eq Hab mini</th>
    <th>Nb Eq Hab maxi</th>
  </tr>
  </thead>
  <% @immeubles.each do |immeuble| %>

    <tr>
    <td><%= immeuble.number %><br/>
        <%= img_boolean(immeuble.number, 'adresse dossier') %></td>
    <td><% immeuble.adresses.each do |imm|%>
            <%= imm.numero_rue %>
            <%= imm.contenu %><br/>
            <%= imm.code_postal %>
            <strong><%= imm.commune.upcase %></strong>

     <%end%>
     </td>
     <td><% immeuble.users.each do |usager| %>
            <%= link_to usager.username, [usager], :class =>"show_link" %> : <%= usager.fonction%>
            <% end %>
     </td>
      <td><%= immeuble.type_habitat %></td>
      <td><%= immeuble.taux_occupation %></td>
      <td><%= immeuble.annee_constr %></td>
      <td><%= immeuble.annee_renov %></td>
      <td><%= immeuble.nb_piece_princ %></td>
      <td><%= immeuble.nb_eq_hab_min %></td>
      <td><%= immeuble.nb_eq_hab_max %></td>
      <td>
      <%= link_to image_tag("icons/eye.png", :alt => "voir", :title => "voir"), [@dossier,immeuble], :class =>"show_link" %>&nbsp;
      <%= link_to image_tag("icons/pencil.png", :alt => "modifier", :title => "modifier"), edit_dossier_immeuble_path(@dossier,immeuble) %>&nbsp;
      <%= link_to image_tag("icons/cross.png", :alt => "détruire", :title => "détruire"), [@dossier,immeuble] , :confirm => 'Certain(e)??', :method => :delete %>
      </td>
      </tr>
  <% end %>
</table>
</div>
