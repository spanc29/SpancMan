<div id="login_form">
<%= form_tag sessions_path do %>
    <p><%= label_tag :login, "Identifiant" %><br />
    <%= text_field_tag :login, params[:login] %></p>
    <p>
    <%= label_tag :password, "mot de passe" %><br />
    <%= password_field_tag :password %></p>
  <p><%= submit_tag "connexion" %></p>
<% end %>
</div>



