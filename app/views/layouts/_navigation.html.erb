    <%= tab.installation 'Installation(s)', dossier_installations_path(@dossier) %>
    <%= tab.entretien 'Entretien(s)', dossier_entretiens_path(@dossier) %>
    <%= tab.controle 'ContrÃ´le(s)', listes_path %>
