<div class="twelve_column section">
  <div class="three column"><%= content_tag :h1, yield(:title) if show_title? %></div>

  <div class="three column"><% @dossier.immeubles.principal.each do |immeu|%>
       <% immeu.users.proprio_destinat.each do |proprio|%> <span class="large"><%= image_tag("icons/users.png", :alt => "Proprio") %> <%= proprio.username %></span></div>
  <div class="three column">
      <%= image_tag("icons/home.png", :alt => "Commune") %> <%= affiche_liste(immeu.commune,'') %>
      <%end%><%end%></div>
  <div class="three column"><span class="small quiet"><%= image_tag("icons/zone.png", :alt => "secteur") %>  <%= affiche_liste(@dossier.secteur,'') %></span></div>
</div>


