<%= form_for @groupe do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :nom %><br />
    <%= f.text_field :nom %>
  </p>
  <p>
    <%= f.label :actif %><br />
    <%= f.check_box :actif %>
  </p>
  <p>
    <%= f.label :agrement %><br />
    <%= f.text_field :agrement %>
  </p>
  <p>
    <%= f.label :date_expiration %><br />
    <%= f.date_select :date_expiration %>
  </p>
  <p>
    <%= f.label :commentaire %><br />
    <%= f.text_area :commentaire %>
  </p>
  <p><%= f.submit %></p>
<% end %>
